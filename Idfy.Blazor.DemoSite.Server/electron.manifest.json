{
  "executable": "Idfy.Blazor.DemoSite.Server",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": false,
  "company":"Idfy Norge",
  "build": {
    "appId": "com.Idfy.Blazor.DemoSite.Server.app",
    "productName": "Signature Demo App",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "1.0.0",
