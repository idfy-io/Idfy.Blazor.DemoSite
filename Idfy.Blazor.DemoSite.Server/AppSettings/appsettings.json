    "GeneralRules": [
      {
        "Endpoint": "*:/api*",
        "Period": "1s",
        "Limit": 4
      },
      {
        "Endpoint": "*:/api*",
        "Period": "15m",
        "Limit": 20
      },
      {
        "Endpoint": "*:/api*",
        "Period": "12h",
        "Limit": 100
      },
      {
        "Endpoint": "*:/api*",
        "Period": "7d",
        "Limit": 200
      }
    ]
  }
